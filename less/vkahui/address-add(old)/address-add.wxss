  margin-left: 60rpx;
  margin-top: 48rpx;
  margin-right: 60rpx;
  line-height: 80rpx;
  border: 1px solid #f74b7b;
  background-color: #f74b7b;
  color: #fff;
}
.azm-btn.azm-btn-code {
  border: 1px solid #f74b7b;
