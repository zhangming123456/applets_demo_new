page {
  background-color: #f1f3f4;
}
.shopFront .bg-color-white {
  background-color: #fff;
}
.shopFront .header swiper {
  height: 292rpx;
}
.shopFront .header swiper .topImages {
  width: 750rpx;
  height: 292rpx;
}
.shopFront .section .topInfo {
  border-bottom: 2rpx solid #e2e2e2;
}
.shopFront .section .topInfo > view {
  height: 80rpx;
  overflow: hidden;
  padding-left: 64rpx;
  margin: 0rpx 42rpx 0rpx 42rpx;
}
.shopFront .section .topInfo > view::after {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 0;
  content: ' ';
  width: 32rpx;
  height: 32rpx;
  background: url("data:image/png;base64,") no-repeat 0 0;
  background-size: 32rpx 32rpx;
}
.shopFront .section .topInfo > view swiper {
  height: 100%;
}
.shopFront .section .topInfo > view swiper .p {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 26rpx;
  color: #6b6a6e;
  line-height: 80rpx;
}
.shopFront .section .shopInfo {
  margin: 0rpx;
}
.shopFront .section .shopInfo-top {
  width: 100%;
  border-bottom: 2rpx solid #e2e2e2;
  padding: 42rpx 0rpx 10rpx 0rpx;
}
.shopFront .section .shopInfo-top .left {
  width: 26.666666666666666%;
  margin-bottom: 42rpx;
}
.shopFront .section .shopInfo-top .left image {
  width: 135rpx;
  height: 135rpx;
  border-radius: 22rpx;
}
.shopFront .section .shopInfo-top .left view {
  text-align: center;
  width: 135rpx;
  height: 135rpx;
  border-radius: 22rpx;
}
.shopFront .section .shopInfo-top .left view .azm-icon {
  color: #7e8396;
  font-size: 90rpx;
}
.shopFront .section .shopInfo-top .right {
  width: 73.33333333333334%;
  height: 100%;
  overflow: hidden;
}
.shopFront .section .shopInfo-top .right .h1 {
  height: 36rpx;
  line-height: 36rpx;
  font-size: 32rpx;
}
.shopFront .section .shopInfo-top .right .p {
  min-height: 32rpx;
  margin-top: 20rpx;
}
.shopFront .section .shopInfo-top .right .p .azm-icon {
  position: absolute;
  top: -6rpx;
  left: 0;
  color: #98979a;
  font-size: 32rpx;
}
.shopFront .section .shopInfo-top .right .p .text {
  line-height: 34rpx;
  padding-left: 44rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  color: #98979a;
}
.shopFront .section .shopInfo-list {
  height: 100%;
  width: 100%;
}
.shopFront .section .shopInfo-list .shopInfo-list-item {
  padding: 30rpx 0rpx 30rpx 0rpx;
  text-align: center;
}
.shopFront .section .shopInfo-list .shopInfo-list-item .text {
  color: #6b6a6e;
}
.shopFront .section .shopInfo-list .shopInfo-list-item.navigator-hover {
  background: #fff;
  opacity: 0.8;
}
.shopFront .section .shopInfo-list .shopInfo-list-item.navigator-hover .text {
  color: #ff0000;
}
.shopFront .section .shopInfo-list .shopInfo-list-item image {
  width: 92rpx;
  height: 92rpx;
  margin-bottom: 20rpx;
}
.shopFront .section .c-grey {
  color: #8e8e8e;
}
.shopFront .section .clubCard {
  margin-top: 18rpx;
}
.shopFront .section .clubCard .h2 {
  top: 0;
  left: 0;
  height: 108rpx;
  line-height: 108rpx;
  padding-left: 64rpx;
  font-size: 30rpx;
  color: #2d2c34;
  border-bottom: 2rpx solid #e2e2e2;
}
.shopFront .section .clubCard .h2::after {
  position: absolute;
  content: '';
  top: 50%;
  transform: translateY(-50%);
  left: 0;
  width: 36rpx;
  height: 30rpx;
  background: url("data:image/png;base64,") no-repeat 0 0;
  background-size: 36rpx 30rpx;
}
.shopFront .section .clubCard .clubCard-info {
  padding: 34rpx 0rpx 34rpx 0rpx;
}
.shopFront .section .clubCard .clubCard-info .p {
  padding-top: 24rpx;
  font-size: 32rpx;
  text-align: center;
  min-height: 74rpx;
}
.shopFront .section .clubCard .clubCard-info .p:nth-of-type(1) {
  color: #2d2c34;
}
.shopFront .section .clubCard .clubCard-info .p:nth-of-type(2) {
  color: #8e8e8e;
  font-size: 26rpx;
}
.shopFront .section .merchants {
  margin-top: 18rpx;
}
.shopFront .section .merchants .h2 {
  top: 0;
  left: 0;
  height: 108rpx;
  line-height: 108rpx;
  padding-left: 64rpx;
  font-size: 30rpx;
  color: #2d2c34;
  border-bottom: 2rpx solid #e2e2e2;
}
.shopFront .section .merchants .h2::after {
  position: absolute;
  content: '';
  top: 50%;
  transform: translateY(-50%);
  left: 0;
  width: 34rpx;
  height: 34rpx;
  background: url("data:image/png;base64,") no-repeat 0 0;
  background-size: 34rpx 34rpx;
}
.shopFront .section .merchants .merchants-info {
  padding: 40rpx 44rpx 100rpx 44rpx;
}
.shopFront .section .merchants .merchants-info > view {
  padding-bottom: 26rpx;
}
.shopFront .section .merchants .merchants-info > view .h3 {
  font-weight: 500;
  font-size: 26rpx;
}
.shopFront .section .merchants .merchants-info > view .ul {
  padding-bottom: 0rpx;
}
.shopFront .section .merchants .merchants-info > view .ul .li {
  color: #8e8e8e;
}
.shopFront .section .merchants .merchants-info > view .p {
  padding-top: 26rpx;
  text-indent: 50rpx;
  color: #8e8e8e;
}
.shopFront .footer {
  width: 100%;
  height: 140rpx;
  padding-left: 218rpx;
  padding-top: 46rpx;
  overflow: hidden;
}
.shopFront .footer .left image {
  width: 136rpx;
  height: 48rpx;
}
.shopFront .footer .right {
  margin-left: 22rpx;
}
.shopFront .footer .right .p {
  height: 30rpx;
  line-height: 30rpx;
  color: #aeb5cd;
  font-size: 24rpx;
}
