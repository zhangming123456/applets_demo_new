}
page view.old-class .cus-height {
  height: 1.1rem;
}
page view.old-class .tab-ctn {
  padding: 2rem 0;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 100;
  border-bottom: 1px solid #ddd;
}
page view.old-class .tab-ctn .bg-color-gray {
  border-radius: 100px;
}
page view.old-class .tab-ctn .tab {
  padding: 0.8rem 1.2rem;
  border-radius: 100px;
}
page view.old-class .tab-ctn .tab .p {
  line-height: 1.1rem;
}
page view.old-class .tab-ctn .tab .circle-text {
  background: #f74b7b;
  min-width: 1.2rem;
  height: 1rem;
  line-height: 1rem;
  border-radius: 1rem;
  color: #fff;
  padding: 0 0.1rem;
}
page view.old-class .tab-ctn .tab .spec {
  position: absolute;
  right: 0;
  top: 0;
  width: 0.4rem !important;
  height: 0.42rem !important;
}
page view.old-class .tab-ctn .tab.active {
  background: #f74b7b;
}
page view.old-class .tab-ctn .tab.active .p {
  color: #fff;
}
page view.old-class .tab-ctn .tab.active .p.circle-text {
  color: #f74b7b;
  background: #fff;
}
page view.old-class .ct {
  text-align: center;
  background: #fff;
  border-radius: 0.3rem;
  box-shadow: 0 1px 0 #dcdcdc;
  margin: 1rem 1.3rem 1rem 1.3rem;
}
page view.old-class .ct .dot {
  width: 100%;
  height: 22rpx;
  top: 190rpx;
  left: 0;
  z-index: 10;
}
page view.old-class .ct .entry {
  width: 100%;
  background: #f74b7b;
  border-top-left-radius: 0.2rem;
  border-top-right-radius: 0.2rem;
  padding: 0.625rem 0;
  position: relative;
}
page view.old-class .ct .entry .img-area {
  width: 30%;
  text-align: center;
  box-sizing: border-box;
}
page view.old-class .ct .entry .img-area .icon-user-bigger image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 100%;
  overflow: hidden;
}
page view.old-class .ct .entry .img-area .azm-icon {
  font-size: 80rpx;
}
page view.old-class .ct .entry .word-area {
  box-sizing: border-box;
  width: 70%;
  text-align: left;
  padding: 0.3rem 0rem 0 0.4rem;
}
page view.old-class .ct .entry .word-area .bottom .inline-div {
  color: #fff;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  max-width: 200rpx;
  min-width: 140rpx;
  vertical-align: text-bottom;
}
page view.old-class .ct .entry .word-area .top {
  position: relative;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}
page view.old-class .ct .entry .word-area .top .shopName {
  max-width: 300rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
page view.old-class .ct .entry .word-area .top .h2 {
  font-size: 1rem;
  width: 100%;
  line-height: 1.5rem;
}
page view.old-class .ct .entry .word-area .top .level-ctn {
  position: absolute;
  right: 0rem;
  top: -1.2rem;
  text-align: center;
}
page view.old-class .ct .entry .word-area .top .level-ctn .level {
  display: block;
}
page view.old-class .ct .entry .word-area .top .level-ctn .level text {
  line-height: 1.1rem;
}
page view.old-class .ct .entry .word-area .bottom .inline-div {
  width: auto;
}
page view.old-class .ct .entry .word-area .bottom .inline-div text {
  color: #fff;
}
page view.old-class .ct .entry > image {
  height: 0.3rem;
  width: 100%;
  position: absolute;
  bottom: 0rem;
  left: 0rem;
}
page view.old-class .level.p {
  margin-top: 0.3rem;
}
page view.old-class .level-b {
  background: #f74b7b;
}
page view.old-class .level-a {
  background: #f74b7b;
}
page view.old-class .bold {
  font-weight: bold;
}
page view.old-class .title {
  overflow: hidden;
}
page view.old-class .dazhe {
  display: inline-block;
  color: #f74b7b;
  background: #fff;
  font-size: 0.8rem;
  padding: 0.05rem 0.3rem;
}
page view.old-class .address {
  font-size: 0.75rem;
  font-weight: normal;
  color: #8e8e8e;
  margin: 0.9rem 0 0.5rem 1rem;
  box-sizing: border-box;
  text-align: left;
}
page view.old-class .number {
  font-size: 0.6875rem;
  font-weight: normal;
  color: #8e8e8e;
  margin: 0rem 1rem 0rem 0rem;
  box-sizing: border-box;
  text-align: right;
}
page view.old-class .line {
  display: inline-block;
  border: 1.98px solid #fff;
  border-radius: 0.3rem;
  width: 1.3rem;
  margin-bottom: 0.3rem;
}
page view.old-class .module {
  display: none;
}
page view.old-class .tanchuang {
  padding: 1rem 1rem 0.7rem 1rem;
  opacity: 1;
  background: #fff;
  position: absolute;
  right: 0%;
  top: 25%;
  z-index: 100;
  margin: 0 2.3rem;
  border-radius: 2px;
}
page view.old-class .tanchuang .content {
  line-height: 1.3rem;
  margin: 0.5rem 0;
  font-size: 0.9rem;
}
page view.old-class .tanchuang .use,
page view.old-class .tanchuang .phone {
  width: 50%;
  background: none;
  border-right: 1px solid #bfc4d3;
  border-radius: 0;
  color: #000;
  font-size: 1rem;
}
page view.old-class .tanchuang .phone {
  border-right: none;
  color: #f74b7b;
}
page view.old-class .tanchuang .h4 {
  border-top: 1px solid #bfc4d3;
}
page view.old-class .moduleActive {
  background: #57565c;
  position: absolute;
  overflow: hidden;
  margin: auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: fixed;
  z-index: 10;
  display: block;
  opacity: 0.95;
}
