}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-box .azm-orderList-li .azm-left .p:nth-of-type(2) {
  color: rgba(45, 44, 52, 0.8);
  font-size: 24rpx;
}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-box .azm-orderList-li .azm-left .p .azm-icon {
  font-size: 30rpx;
}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-box .azm-orderList-li .azm-left .p .azm-icon.azm-icon-blue {
  color: #008de6;
}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-box .azm-orderList-li .azm-left .p .azm-icon.azm-icon-pink {
  color: #f74b7b;
}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-box .azm-orderList-li .azm-center {
