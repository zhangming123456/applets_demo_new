}
page view.new-class .azm-orderInfo .box .azm-open-module {
  top: 50%;
  transform: translateY(-50%);
  right: 0;
  height: 160rpx;
  line-height: 160rpx;
  width: 100%;
  text-align: right;
}
page view.new-class .azm-orderInfo .box .azm-open-module .azm-icon {
