}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-title text {
  font-size: 24rpx;
}
page view.new-class .azm-order .azm-orderList .azm-orderList-item .azm-orderList-title .flexBox-item {
