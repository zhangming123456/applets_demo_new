page view.old-class .list-3 .entry {
  font-size: 0.85rem;
  border-bottom: 2px solid #f1f3f4;
  padding: 0.5rem 0;
  margin: 0 0.5rem;
}
page view.old-class .p {
  color: #aab1cb;
}
page view.old-class .ct.cus-1 {
  text-align: center;
  min-height: 6rem;
  position: relative;
  background: #fff;
}
page view.old-class .ct.cus-1 .entry {
  width: 90%;
  border-radius: 0.5rem;
  height: 7rem;
  padding: 0.8rem 0;
  position: absolute;
  top: -3rem;
  left: 5%;
}
page view.old-class .ct.cus-1 .entry .img-area {
  height: 7rem;
  width: 30%;
  text-align: center;
  padding-left: 0.8rem;
  box-sizing: border-box;
}
page view.old-class .ct.cus-1 .entry .img-area image {
  width: 5.5rem !important;
  height: 5.5rem !important;
  border-radius: 300px;
}
page view.old-class .ct.cus-1 .entry .word-area {
  height: 7rem;
  box-sizing: border-box;
  width: 66%;
  text-align: left;
  padding: 0 1.2rem 0 0.8rem;
}
page view.old-class .ct.cus-1 .entry .word-area .top {
  position: relative;
  height: 3rem;
}
page view.old-class .ct.cus-1 .entry .word-area .top .h2 {
  font-weight: bold;
}
page view.old-class .ct.cus-1 .entry .word-area .top .level-ctn {
  position: absolute;
  right: 0;
  top: 0;
  text-align: center;
}
page view.old-class .ct.cus-1 .entry .word-area .top .level-ctn .level {
  display: block;
}
page view.old-class .ct.cus-1 .entry .word-area .top .level-ctn .level text {
  line-height: 1.1rem;
}
page view.old-class .ct.cus-1 .entry .word-area .bottom .inline-div {
  width: auto;
}
page view.old-class .ct.cus-1 .entry .word-area .bottom .inline-div text {
  color: #a3aab8;
}
page view.old-class .arrow-list-ctn .list .entry {
  padding-left: 0;
}
page view.old-class .arrow-list-ctn .fr {
  float: right !important;
  padding-right: 1rem;
}
page view.old-class .arrow-list-ctn .list .entry:last-child {
