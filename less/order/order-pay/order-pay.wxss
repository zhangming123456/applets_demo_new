  padding-right: 1rem;
  padding-left: 1.5rem;
  margin: 0;
  border-bottom: none;
}
page view.old-class .entry > image {
  width: 1.3rem;
  height: 1.2rem;
  margin: 0.1rem 1.2rem 0 0;
}
page view.old-class .line {
  border-top: 8px solid #f1f3f4;
}
page view.old-class .line-bottom {
  border-bottom: 8px solid #f1f3f4;
}
page view.old-class .card {
  font-size: 0.875rem;
  color: #aab1cb;
  margin: -0.3rem 1.5rem 0rem 3.75rem;
  border-top: 1px solid #f1f3f4;
  padding: 0.5rem 0;
}
page view.old-class .card .balance {
  float: right;
}
page view.old-class .triangle {
  width: 0.7rem;
  height: 0.5rem;
  position: absolute;
  right: 0rem;
  top: 0.8rem;
}
page view.old-class .menu {
  background: #fff;
  font-size: 0.8rem;
  width: 100%;
  color: #aab1cb;
  margin-left: 0.6rem;
  box-sizing: border-box;
  margin-right: 0.5rem;
}
page view.old-class .menu .detail {
  margin: 0.7rem 0;
  padding-right: 4.5rem;
}
page view.old-class .show {
