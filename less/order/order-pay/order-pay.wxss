}
page view.old-class .module {
  display: none;
}
page view.old-class .tanchuang {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 520rpx;
  padding: 0.5rem 1rem 0.2rem 1rem;
  opacity: 1;
  background: #fff;
  z-index: 100;
  border-radius: 2px;
}
page view.old-class .tanchuang .content {
  line-height: 1.3rem;
  margin: 0.5rem 0;
  font-size: 0.9rem;
}
page view.old-class .tanchuang .use,
page view.old-class .tanchuang .phone {
  width: 50%;
  background: none;
  border-right: 1px solid #bfc4d3;
  border-radius: 0;
  color: #000;
  font-size: 1rem;
}
page view.old-class .tanchuang .phone {
  border-right: none;
  color: #f74b7b;
}
page view.old-class .tanchuang .h4 {
  border-top: 1px solid #bfc4d3;
}
page view.old-class .moduleActive {
  background: #57565c;
  position: absolute;
  right: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
  display: block;
  opacity: 1;
}
page view.old-class .message {
  display: none;
}
page view.old-class .tanchuangMe {
  margin: 1.656rem 1.3125rem 1.4375rem 1.3125rem;
